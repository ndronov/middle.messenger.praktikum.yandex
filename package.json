{
  "name": "middle.messenger.praktikum.yandex",
  "version": "4.0.0",
  "description": "Messenger application",
  "main": "",
  "target": "web",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config webpack.config.babel.js",
    "build": "webpack --progress --config webpack.config.babel.js",
    "start": "npm run build && node server",
    "push:container": "heroku container:push web --app=messenger-domain",
    "release:container": "heroku container:release web --app=messenger-domain",
    "deploy": "npm run push:container && npm run release:container",
    "lint": "eslint --ignore-path .gitignore src/**/*.ts",
    "stylelint": "stylelint --ignore-path .gitignore src/**/*.scss",
    "test": "mocha --require ts-node/register src/**/*.spec.ts"
  },
  "repository": {
