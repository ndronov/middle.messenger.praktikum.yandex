{
  "name": "middle.messenger.praktikum.yandex",
  "version": "4.0.0",
  "description": "Messenger application",
  "scripts": {
    "start:webpack": "npm run build:dev",
    "build:dev": "webpack-dev-server --env dev --mode none",
    "build:prod": "webpack --env prod --mode production",
    "prebuild:parcel": "rimraf build/*",
    "build:parcel": "parcel build ./src/app.html --no-scope-hoist --dist-dir build",
    "dev:parcel": "parcel serve ./src/app.html --open --dist-dir .parcel-serve",
    "prestart:parcel": "rimraf build/*",
    "start:parcel": "npm run build:parcel && node server",
    "start": "npm run start:parcel",
    "server": "node server",
    "lint": "eslint --ext .ts src",
    "stylelint": "stylelint src/**/*.scss",
    "test": "mocha --require ts-node/register src/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ndronov/middle.messenger.praktikum.yandex.git"
  },
  "author": "Nikita Dronov",
  "license": "ISC",
