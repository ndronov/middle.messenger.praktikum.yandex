    "@types/html-webpack-plugin": "^3.2.6",
    "@types/jsdom": "^16.2.13",
    "@types/mini-css-extract-plugin": "^2.4.0",
    "@types/mocha": "^9.0.0",
    "@types/sanitize-html": "^2.5.0",
    "@types/sinon": "^10.0.6",
    "@types/webpack": "^5.28.0",
