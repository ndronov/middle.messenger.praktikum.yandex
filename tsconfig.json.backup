{
  "compilerOptions": {
    "outDir": "./dist/",
    "downlevelIteration": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "ES6",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  }
}
